<body class="SignUp">
<h2>Sign up</h2>

<%= form_for @user, html: { multipart: true } do |form| %>
  <% if @user.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@user.errors.count, "error") %> prohibited this user from being saved:</h2>

      <ul>
      <% @user.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
   

  <div class="field">
    <%= form.label :First %>
    <%= form.text_field :fname, id: :user_fname %>
  </div>

  <div class="field">
    <%= form.label :Last %>
    <%= form.text_field :lname, id: :user_lname %>
  </div>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email, id: :user_email %>
  </div>

  <div class="field">
    <%= form.label :location %>
    <%= form.text_field :location, id: :user_location %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.text_field :password, id: :user_password %>
  </div>

  <div class="field">
    <%= form.label :interest %>
    <%= form.text_field :interest, id: :user_interest %>
  </div>
  <div class="field">
    <%= form.label :Resume %>
    <%= form.text_field :resume, id: :user_resume %>
  </div>
   <div class="field">
    <%= form.label :Cover_Letter %>
    <%= form.text_field :cover_letter, id: :user_cover_letter %>
  </div>

   <div class="field">
    <%= form.label :photo %>
    <%= form.file_field :photo, id: :user_photo %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>


<%= render "devise/shared/links" %>
</body>
